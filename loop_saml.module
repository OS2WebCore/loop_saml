<?php

/**
 * @param $settings
 */
function loop_saml_saml_sp_settings_alter(&$settings) {
  $settings['debug'] = TRUE;
  $settings['strict'] = FALSE;
  $settings['security']['allowedAuthContexts'] = FALSE;
  $settings['security']['logoutRequestSigned'] = TRUE;
  $settings['security']['logoutResponseSigned'] = TRUE;
}
